#include <stdio.h>
int f(int n) {
    if(n == 0)
        return 1;
    return f(n - 1) + f(n - 1);
}
int main() {
    int n, i, j;
    scanf("%d", &n);
    f(n);
    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            int a = 0;
            a++;
        }
    }
    for(i = 0; i < 7; i++) {
        int b = 0;
        b++;
    }
    return 0;
}
