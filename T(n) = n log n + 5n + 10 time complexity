#include <stdio.h>
int main() {
    int n, i, j;
    scanf("%d", &n);
    for(i = 1; i <= n; i++) {
        for(j = i; j > 1; j = j / 2) {
            int a = 0;
            a++;
        }
    }
    for(i = 0; i < n; i++) {
        int b = 0;
        b++; b++; b++; b++; b++;
    }
    for(i = 0; i < 10; i++) {
        int c = 0;
        c++;
    }
    return 0;
}
