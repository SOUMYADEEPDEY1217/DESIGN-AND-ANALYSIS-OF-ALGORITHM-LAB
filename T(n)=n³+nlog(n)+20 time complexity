#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            for(int k = 0; k < n; k++) {
                int x = i + j + k;   
            }
        }
    }
    for(int i = 1; i <= n; i++) {
        for(int j = 1; j <= n; j = j * 2) {
            int y = i + j;    
        }
    }
    for(int i = 0; i < 20; i++) {
        int z = i;
    }
    return 0;
}
