#include <stdio.h>
int main() {
    int a[100][100], b[100][100], result[100][100];
    int r1, c1, r2, c2, i, j, k;
    printf("Enter Rows and Cols for Matrix A: ");
    scanf("%d %d", &r1, &c1);
    printf("Enter Rows and Cols for Matrix B: ");
    scanf("%d %d", &r2, &c2);
    if (c1 != r2) {
        printf("Cannot multiply! Columns of A must match Rows of B.\n");
        return 0;
    }
    printf("Enter elements of Matrix A:\n");
    for (i = 0; i < r1; i++)
        for (j = 0; j < c1; j++)
            scanf("%d", &a[i][j]);
    printf("Enter elements of Matrix B:\n");
    for (i = 0; i < r2; i++)
        for (j = 0; j < c2; j++)
            scanf("%d", &b[i][j]);
    printf("\nResultant Matrix:\n");
    for (i = 0; i < r1; i++) {
        for (j = 0; j < c2; j++) {
            result[i][j] = 0; 
            for (k = 0; k < c1; k++) {
                result[i][j] += a[i][k] * b[k][j];
            }
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }
    return 0;
}
